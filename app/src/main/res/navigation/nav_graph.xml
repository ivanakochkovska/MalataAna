        <action
            android:id="@+id/action_secondFragment_to_menuChooseEmotionFragment"
            app:destination="@id/menuChooseEmotionFragment" />
    </fragment>
    <fragment
        android:id="@+id/happinessFragment"
        android:name="com.example.malataana.HappinessFragment"
