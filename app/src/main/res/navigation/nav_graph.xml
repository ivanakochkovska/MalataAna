        <action
            android:id="@+id/action_angerQuizFragment_to_mainMenuFragment"
            app:destination="@id/mainMenuFragment" />
        <action
            android:id="@+id/action_angerQuizFragment_to_happinessQuizFragment"
            app:destination="@id/happinessQuizFragment" />
    </fragment>
